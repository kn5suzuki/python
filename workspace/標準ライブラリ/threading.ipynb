   "source": [
    "import time\n",
    "def f():\n",
    "    time.sleep(1)\n",
    "    print('slept in thread')\n",
    "\n",
    "thread = threading.Thread(target=f)\n",
    "thread.start()\n",
    "print('outside')\n",
    "time.sleep(2)\n",
    "print('slept outside')"
   ]
